<%= simple_form_for(@trainee) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :last_name %>
    <%= f.input :notes %>
    <%= f.input :end_suscription_date %>
    <%= f.association :trainer %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
